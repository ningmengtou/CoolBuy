.up_img_wrap {
    width: 100rpx;
    height: 100rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
    margin: 20rpx;
    margin-left: 0;
}

.up_img_wrap image {
    width: 100%;
    height: 100%;
    border-radius: 10rpx;
}

.up_img_wrap icon {
    position: absolute;
    top: 0;
    right: 0;
    transform: translate(30%, -30%);
}